	<IconTabFilter text="Closed" id="ClosedRegITab" key="C" icon="sap-icon://complete">
	</IconTabFilter>
	</items>
	</IconTabBar>
	
	<core:Fragment id="RegsTableFragment" fragmentName="sap.ui.project.e2etm.fragment.Registration.RegistrationTable" 
					type="XML" /> 
	
	</content>
	</Page>
	</core:View>